294 \ud300\uc785\ub2c8\ub2e4. \n\uc2e4\ubb34 \uc5ed\ub7c9\uacfc \ucf58\ud150\uce20 \uc5ed\ub7c9\uc744 \ubaa8\ub450 \uac16\ucd98 \ud6cc\ub96d\ud55c \ud300\uc6d0\ub4e4\uc774 \ubaa8\uc5ec \uc218\uac15\uc0dd \ubd84\ub4e4\uc774 \ubcf4\ub2e4 \uc27d\uac8c \uc774\ud574\ud558\uace0, \ubcf4\ub2e4 \uc7ac\ubbf8\uc788\uac8c \ubc30\uc6b8 \uc218 \uc788\ub3c4\ub85d \ud56d\uc0c1 \uace0\ubbfc\ud569\ub2c8\ub2e4. \uadf8\ub9ac\uace0 \uae4a\uc740 \uace0\ubbfc \ub05d\uc5d0 \uac00\uc7a5 \ud544\uc694\ud55c \uc8fc\uc81c\ub97c \uc120\uc815\ud558\uc5ec \ubaa8\ub4e0 \ub808\uc2a8\uc774 \uc720\uae30\uc801\uc73c\ub85c \uc5f0\uacb0\ub420 \uc218 \uc788\ub3c4\ub85d \ucee4\ub9ac\ud058\ub7fc\uc744 \uae30\ud68d\ud558\uace0 \ucf58\ud150\uce20\ub97c \uc81c\uc791\ud569\ub2c8\ub2e4. \ub2e8\uc21c\ud788 \ubc30\uc6b0\ub294 \uac83\uc5d0 \uadf8\uce58\ub294 \uac83\uc774 \uc544\ub2cc, \uc0ac\ub78c\ub4e4\uc774 \ubc30\uc6c0\uc758 \uae30\uc068\uc744 \ub290\ub07c\uace0 \ubc30\uc6c0\uc5d0 \ube60\uc9c0\ub3c4\ub85d \ud569\ub2c8\ub2e4.", "requirements": "\u2022 \uae00\uc744 \uc798 \uc4f0\uc2dc\ub294 \ubd84\n\u2022 \ub2e4\uc591\ud55c \ubd84\uacfc \uc6d0\ud65c\ud558\uac8c \ud611\uc5c5\ud560 \uc218 \uc788\ub294 \ubd84\n\u2022 \uac00\ub974\uce58\ub294 \uc13c\uc2a4\uc640 \ucf58\ud150\uce20 \uac10\uac01\uc774 \uc788\ub294 \ubd84", "job_country": "KR", "industry": "IT, \ucee8\ud150\uce20", "company_id": 1939, "title_img": "https://static.wanted.co.kr/images/company/1939/swzwjygoxj6izlei__1080_790.jpg", "due_time": null, "location": "\uc911\uad6c \uccad\uacc4\ucc9c\ub85c 100 \uc2dc\uadf8\ub2c8\uccd0\ud0c0\uc6cc \ub3d9\uad00 10\uce35", "is_expert": true, "sub_categories": ["Back-end Engineer", "\uc11c\ubc84 \uac1c\ubc1c\uc790", "Web Developer", "\uc6f9 \uac1c\ubc1c\uc790"], "position": "\ud14c\ud06c \ucf58\ud150\uce20 \uc5d0\ub514\ud130", "reward": 1000000, "jd": "\u2605 2022 \ucf54\ub4dc\uc787 \uc778\uc7ac \ucc44\uc6a9: \uc804 \uc9c1\uad70 \uc2a4\ud1a1\uc635\uc158 \uc81c\uacf5 \u2605\n\n\n\uc8fc\uc694\uc5c5\ubb34\n\ud604\uc7ac \ucf54\ub4dc\uc787\uc740 \ubaa8\ub4e0 \ucf58\ud150\uce20\ub97c \uc790\uccb4 \uc81c\uc791\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4. \ud558\uc9c0\ub9cc \ub54c\ub85c\ub294 \ubd84\uc57c\uac00 \ube60\ub974\uac8c \ubcc0\ud574\uc11c, \uc804\ubb38 \uc9c0\uc2dd\uc774 \ud544\uc694\ud574\uc11c, \uc218\uc694\uac00 \uc881\uc544\uc11c \ub4f1\uc758 \uc774\uc720\ub85c \uc790\uccb4 \uc81c\uc791\uc774 \uc5b4\ub824\uc6b4 \uc8fc\uc81c\uac00 \uc788\ub294\ub370\uc694. \uc774\ub7f0 \uc8fc\uc81c\ub294 \uc2e4\ubb34 \uc5ed\ub7c9\uacfc \ucf58\ud150\uce20 \uc5ed\ub7c9\uc744 \uac00\uc9c4 \ucf58\ud150\uce20 \ud30c\ud2b8\ub108\ub97c \ucc3e\uace0 \ud611\uc5c5\ud558\uc5ec \uc81c\uc791\ud558\ub824 \ud569\ub2c8\ub2e4.\n\n\u2022 \ud300\uc5d0\uc11c \uc120\uc815\ud55c \uc8fc\uc81c\uc758 \ucf54\ub4dc\uc787 \ucf58\ud150\uce20\ub97c \uc798 \ub9cc\ub4e4 \uc218 \uc788\ub294 \ucf58\ud150\uce20 \ud30c\ud2b8\ub108\ub97c \ubc1c\uad74\ud569\ub2c8\ub2e4.\n\u2022 \ucf58\ud150\uce20 \ud30c\ud2b8\ub108\uc640 \ud611\uc5c5\ud558\uc5ec \ucf58\ud150\uce20\ub97c \uc81c\uc791\ud569\ub2c8\ub2e4.\n\u2022 \ub9cc\ub4e4\uc5b4\uc9c4 \ucf58\ud150\uce20\ub97c \ucf54\ub4dc\uc787 \uc2a4\ud0c0\uc77c\uc5d0 \ub9de\ub3c4\ub85d \ud55c\uce35 \ub354 \uac1c\uc120\ud569\ub2c8\ub2e4.\n\n\u3010 \uc774\ub7f0 \ud300\uc5d0\uc11c \uc77c\ud574\uc694 \u3011\n\ucf58\ud150\uce20 \ud300\uc740 \ud604 \uc2dc\ub300\uc758 \uad50\uc721\uc758 \ubb38\uc81c\uc810\uc744 \uc9c1\uba74\ud558\uace0, \uc774\ub97c \ud574\uacb0\ud558\uae30 \uc704\ud574 \uac00\uc7a5 \uc55e\ub2e8\uc5d0 \uc788\ub294 \ud300\uc785\ub2c8\ub2e4. \n\uc2e4\ubb34 \uc5ed\ub7c9\uacfc \ucf58\ud150\uce20 \uc5ed\ub7c9\uc744 \ubaa8\ub450 \uac16\ucd98 \ud6cc\ub96d\ud55c \ud300\uc6d0\ub4e4\uc774 \ubaa8\uc5ec \uc218\uac15\uc0dd \ubd84\ub4e4\uc774 \ubcf4\ub2e4 \uc27d\uac8c \uc774\ud574\ud558\uace0, \ubcf4\ub2e4 \uc7ac\ubbf8\uc788\uac8c \ubc30\uc6b8 \uc218 \uc788\ub3c4\ub85d \ud56d\uc0c1 \uace0\ubbfc\ud569\ub2c8\ub2e4. \uadf8\ub9ac\uace0 \uae4a\uc740 \uace0\ubbfc \ub05d\uc5d0 \uac00\uc7a5 \ud544\uc694\ud55c \uc8fc\uc81c\ub97c \uc120\uc815\ud558\uc5ec \ubaa8\ub4e0 \ub808\uc2a8\uc774 \uc720\uae30\uc801\uc73c\ub85c \uc5f0\uacb0\ub420 \uc218 \uc788\ub3c4\ub85d \ucee4\ub9ac\ud058\ub7fc\uc744 \uae30\ud68d\ud558\uace0 \ucf58\ud150\uce20\ub97c \uc81c\uc791\ud569\ub2c8\ub2e4. \ub2e8\uc21c\ud788 \ubc30\uc6b0\ub294 \uac83\uc5d0 \uadf8\uce58\ub294 \uac83\uc774 \uc544\ub2cc, \uc0ac\ub78c\ub4e4\uc774 \ubc30\uc6c0\uc758 \uae30\uc068\uc744 \ub290\ub07c\uace0 \ubc30\uc6c0\uc5d0 \ube60\uc9c0\ub3c4\ub85d \ud569\ub2c8\ub2e4.\n\n\uc790\uaca9\uc694\uac74\n\u2022 \uae00\uc744 \uc798 \uc4f0\uc2dc\ub294 \ubd84\n\u2022 \ub2e4\uc591\ud55c \ubd84\uacfc \uc6d0\ud65c\ud558\uac8c \ud611\uc5c5\ud560 \uc218 \uc788\ub294 \ubd84\n\u2022 \uac00\ub974\uce58\ub294 \uc13c\uc2a4\uc640 \ucf58\ud150\uce20 \uac10\uac01\uc774 \uc788\ub294 \ubd84\n\n\uc6b0\ub300\uc0ac\ud56d\n\u2022 \ucf54\ub4dc\uc787 \ucf58\ud150\uce20 \uc2a4\ud0c0\uc77c\uc744 \uc798 \uc774\ud574\ud558\uc2dc\ub294 \ubd84\n\u2022 \uad50\uc721 \ubd84\uc57c\uc5d0 \uad00\uc2ec\uc774 \ub9ce\uc740 \ubd84\n\u2022 \uae30\uc220 \uc8fc\uc81c\ub97c \uc798 \uc774\ud574\ud560 \uc218 \uc788\uc73c\uc2e0 \ubd84\n\n\n\u3010 \uc774\ub807\uac8c \ud569\ub958\ud558\uac8c \ub3fc\uc694 \u3011\n1. \uc11c\ub958 \uc811\uc218 \u2192 2. \uc0ac\uc804\uacfc\uc81c \u2192 3. \uc778\ud130\ubdf0 \u2192 4. \ud569\ub958 \n\u2022 \uc11c\ub958 \ud1b5\uacfc \uc2dc \ucf58\ud150\uce20 \uc81c\uc791 \uc5ed\ub7c9\uc744 \uac80\uc99d\ud558\uae30 \uc704\ud55c \uacfc\uc81c\ub97c \ub4dc\ub9ac\uace0 \uc788\uc5b4\uc694. (*\uc0ac\uc804\uacfc\uc81c\ube44 \uc81c\uacf5)\n\u2022 \uc9c1\ubb34\uc640 \uceec\uccd0\ud54f \uad00\ub828 \uc778\ud130\ubdf0\uac00 \uc9c4\ud589\ub418\uba70, \ubaa8\ub4e0 \uc778\ud130\ubdf0\ub97c \ud558\ub8e8 \ub3d9\uc548 \uc9c4\ud589\ud574\uc694.\n\u2022 \uc778\ud130\ubdf0\ub294 \ucf54\ub4dc\uc787 \uc0ac\ubb34\uc2e4\uc5d0\uc11c \uc9c4\ud589\ub3fc\uc694.\n\n\u3010 \uc81c\ucd9c \uc11c\ub958 \u3011\n\u2022 (\ud544\uc218) \uc774\ub825\uc11c\n\u2022 (\uc120\ud0dd) \uae00\uc4f0\uae30 \uc5e